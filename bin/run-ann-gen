#!/usr/bin/env python
import sys
import os
import imp
sys.path.append(os.path.abspath(os.path.join(os.path.dirname(__file__), "..")))
durableChannel = imp.load_source("durableChannel", os.path.abspath(os.path.join(os.path.dirname(__file__), "../lib/muppet/durable-channel.py")))

options = {"name": "agg-channel"}
dispatcher = durableChannel.DurableChannel(options)
options["name"] = "worker-1"
worker = durableChannel.durableChannel(options)
dispatcher.send()
dispatcher.close()